{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "1.0.0",
    "tasks": [
        {
            "label": "convert_to_png",
            "type": "shell",
            "command": "conda",
            "args": [
                "run",
                "-n", "wsi",
                "python",
                "-m", "mil_wsi.scripts.convert_to_png",
                "--source", "./mil_wsi/data",
                "--save_dir", "./mil_wsi/data/images/",
                "--file_extension", "svs"

            ],
            "problemMatcher": []
        },
        {
            "label": "create_patches",
            "type": "shell",
            "command": "conda",
            "args": [
                "run",
                "-n", "wsi",
                "python",
                "-m", "mil_wsi.scripts.create_patches",
                "--source", "./mil_wsi/data",
                "--save_dir", "./mil_wsi/results/",
                "--patch_size", "256",
                "--seg",
                "--patch",
                "--stitch",
            ],
            "problemMatcher": []
        },
        {
            "label": "extract_features",
            "type": "shell",
            "command": "conda",
            "args": [
                "run",
                "-n", "wsi",
                "CUDA_VISIBLE_DEVICES=0",
                "python",
                "-m", "mil_wsi.scripts.extract_features",
                "--data_h5_dir", "./mil_wsi/results/",
                "--data_slide_dir", "./mil_wsi/data/",
                "--csv_path", "./mil_wsi/results/process_list_autogen.csv",
                "--feat_dir", "./mil_wsi/results/",
                "--model_name", "conch_v1",
                "--batch_size", "512",
                "--slide_ext", ".svs"
            ],
            "options": {
                "env": {
                    "UNI_CKPT_PATH": "./mil_wsi/UNI/pytorch_model.bin"
                }
            },
            "problemMatcher": []
        },
        {
            "label": "mlp_model",
            "type": "shell",
            "command": "conda",
            "args": [
                "run",
                "-n", "wsi",
                "CUDA_VISIBLE_DEVICES=0",
                "python",
                "-m", "mil_wsi.scripts.mlp_model",
                "--dir_results", "./mil_wsi/results/",
                "--dir_data", "./mil_wsi/data/",
                "--dir_model", "./mil_wsi/models/",
                "--dir_metrics", "./mil_wsi/metrics/",
                "--epochs", "100"
            ],
            "problemMatcher": []
        },
        {
            "label": "mil_model",
            "type": "shell",
            "command": "conda",
            "args": [
                "run",
                "-n", "wsi",
                "CUDA_VISIBLE_DEVICES=0",
                "python",
                "-m", "mil_wsi.scripts.mil_model",
                "--dir_results", "./mil_wsi/results/",
                "--dir_data", "./mil_wsi/data/",
                "--dir_model", "./mil_wsi/models/",
                "--dir_metrics", "./mil_wsi/metrics/",
                "--suffix_name", "_mil"

            ],
            "problemMatcher": []
        },
        {
            "label": "feature_projection",
            "type": "shell",
            "command": "conda",
            "args": [
                "run",
                "-n", "wsi",
                "python",
                "-m", "mil_wsi.scripts.feature_projection",
                "--dir_results", "./mil_wsi/results/",
                "--dir_data", "./mil_wsi/data/"
            ],
            "problemMatcher": []
        },
        {
            "label": "attention_mil_model",
            "type": "shell",
            "command": "conda",
            "args": [
                "run",
                "-n", "wsi",
                "CUDA_VISIBLE_DEVICES=0",
                "python",
                "-m", "mil_wsi.scripts.attention_mil_model",
                "--dir_results", "./mil_wsi/results/",
                "--dir_data", "./mil_wsi/data/",
                "--dir_model", "./mil_wsi/models/",
                "--dir_metrics", "./mil_wsi/metrics/",
                "--suffix_name", "_attention_mil",
            ],
            "problemMatcher": []
        }
        {
            "label": "pipeline",
            "type": "shell",
            "dependsOn": [
                "create_patches",
                "extract_features",
                "mlp_model",
                "mil_model",
                "attention_mil_model"
            ],
            "dependsOrder": "sequence",
            "problemMatcher": []
        }
        
    ]
}