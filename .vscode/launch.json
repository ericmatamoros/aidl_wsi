{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "convert_to_png",
            "type": "python",
            "request": "launch",
            "module": "mil_wsi.scripts.convert_to_png",
            "console": "internalConsole",
            "subProcess": true,
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2"
            },
            "args": [
                "--source", "./mil_wsi/data",
                "--save_dir", "./mil_wsi/data/images/",
                "--file_extension", "svs"
            ]
        },
        {
            "name": "create_patches",
            "type": "python",
            "request": "launch",
            "module": "mil_wsi.scripts.create_patches",
            "console": "internalConsole",
            "subProcess": true,
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2"
            },
            "args": [
                "--source", "./mil_wsi/data",
                "--save_dir", "./mil_wsi/data/results/",
                "--patch_size", "256",
                "--patch",
                "--stitch"
            ]
        },
        {
            "name": "extract_features",
            "type": "python",
            "request": "launch",
            "module": "mil_wsi.scripts.extract_features",
            "console": "internalConsole",
            "subProcess": true,
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2",
                "CUDA_VISIBLE_DEVICES": "0",
                "UNI_CKPT_PATH": "./mil_wsi/UNI/pytorch_model.bin"
            },
            "args": [
                "--data_h5_dir", "./mil_wsi/results/",
                "--data_slide_dir", "./mil_wsi/data/",
                "--csv_path", "./mil_wsi/results/process_list_autogen.csv",
                "--feat_dir", "./mil_wsi/results/",
                "--batch_size", "512",
                "--slide_ext", ".svs"
            ]
        },
        {
            "name": "mlp_model",
            "type": "python",
            "request": "launch",
            "module": "mil_wsi.scripts.mlp_model",
            "console": "internalConsole",
            "subProcess": true,
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2",
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "args": [
                "--dir_results", "./mil_wsi/results/",
                "--dir_data", "./mil_wsi/data/",
                "--dir_model", "./mil_wsi/models/",
                "--dir_metrics", "./mil_wsi/metrics/",
                "--suffix_name", "_mlp"
            ]
        },
        {
            "name": "mil_model",
            "type": "python",
            "request": "launch",
            "module": "mil_wsi.scripts.mil_model",
            "console": "internalConsole",
            "subProcess": true,
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2",
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "args": [
                "--dir_results", "./mil_wsi/results/",
                "--dir_data", "./mil_wsi/data/",
                "--dir_model", "./mil_wsi/models/",
                "--dir_metrics", "./mil_wsi/metrics/",
                "--suffix_name", "_mil"
            ]
        },
        {
            "name": "attention_mil_model",
            "type": "python",
            "request": "launch",
            "module": "mil_wsi.scripts.weighted_model",
            "console": "internalConsole",
            "subProcess": true,
            "env": {
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "2",
                "CUDA_VISIBLE_DEVICES": "0"
            },
            "args": [
                "--dir_results", "./mil_wsi/results/",
                "--dir_data", "./mil_wsi/data/",
                "--dir_model", "./mil_wsi/models/",
                "--dir_metrics", "./mil_wsi/metrics/",
                "--batch_size", "1",
                "--hidden_size", "128",
                "--epochs", "5",
                "--test_size", "0.3",
            ]
        }
    ]
}